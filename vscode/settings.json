{
  "workbench.editor.enablePreview": false,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "window.zoomLevel": 0,
  "breadcrumbs.enabled": true,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "[typescript]": {
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[tsx]": {
    "editor.tabSize": 2
  },
  "[css]": {
    "editor.tabSize": 2
  },
  "[scss]": {
    "editor.tabSize": 2
  },
  "[sass]": {
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.tabSize": 2
  },
  "[ruby]": {
    "editor.tabSize": 2
  },
  "[python]": {
    "editor.tabSize": 2
  },
  "[erb]": {
    "editor.tabSize": 2
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false
  },
  "[yaml]": {
    "editor.tabSize": 2
  },
  "[terraform]": {
    "editor.tabSize": 2
  },
  "liveshare.featureSet": "insiders",
